{
    "name": "shahmarasy/swish-php",
    "description": "Production-grade PHP SDK for the Swish Payment Gateway with mTLS support",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "swish",
        "payment",
        "gateway",
        "sdk",
        "php",
        "mtls",
        "fintech",
        "sweden"
    ],
    "homepage": "https://github.com/shahmarasy/swish-php",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Swish\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swish\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-text"
    }
}